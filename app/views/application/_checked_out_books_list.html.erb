<table class="pure-table pure-table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th>Student</th>
      <th>Student Contact</th>
      <th>Due</th>
    </tr>
  </thead>
  <% @checked_out_books.each do |book| %>
    <tr>
      <td><%= book.id %></td>
      <td><%= book.title %></td>
      <td><%= book.student.name %></td>
      <td><%= book.student.contact %></td>
      <td><%= book.due.strftime('%Y/%_m/%e %A') %></td>
    </tr>
  <% end %>
</table>
