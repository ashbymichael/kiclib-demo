<%- headers = ['ID', 'NUMBER', 'TITLE', 'DUE', 'BORROWER', 'CONTACT'] -%>
<%= CSV.generate_line headers %>
<%- @books.each do |book| -%>
  <%= CSV.generate_line([book.id, book.number, book.title,
                        book.due, (book.student.name if book.student),
                        (book.student.contact if book.student)]) -%>
<%- end -%>
